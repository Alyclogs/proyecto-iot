{
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        }
    ],
    "builds": [
        {
            "src": "client/vite.config.ts",
            "use": "@vercel/vite"
        },
        {
            "src": "api/**/*.js",
            "use": "@vercel/node"
        }
    ]
}